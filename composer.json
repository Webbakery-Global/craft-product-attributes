{
  "name": "the-web-bakery/product-attribute-fields",
  "description": "Creating entry type fields by a product and the values of a entry by the variant",
  "version": "1.0.0",
  "type": "craft-plugin",
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "thewebbakery\\productattributefields\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Casper van Schooten",
      "homepage": "https://github.com/Webbakery-Global/craft-product-attributes"
    }
  ],
  "support": {
    "docs": "https://github.com/Webbakery-Global/craft-product-attributes/blob/main/README.md",
    "issues": "https://github.com/Webbakery-Global/craft-product-attributes/issues",
    "source": "https://github.com/Webbakery-Global/craft-product-attributes"
  },
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.4.0"
  },
  "extra": {
    "handle": "product-attributes-fields",
    "name": "Product Attribute Fields",
    "developer": "The Web Bakery",
    "documentationUrl": "https://github.com/Webbakery-Global/craft-product-attributes/blob/main/README.md",
    "class": "thewebbakery\\productattributefields\\ProductAttributesField"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "https://github.com/Webbakery-Global/craft-product-attributes"
    }
  ],
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
